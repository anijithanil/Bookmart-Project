
        <table class="table">
            <thead>
                <tr>
                    <th>No</th>
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                     <th scope="col"></th>
                </tr>
            </thead>
            <tbody>
                {{#each users}}
                <tr>
                    <td>{{@key}}</td>
                    <td>{{this.name}}</td>
                    <td>{{this.email}}</td>
                    <td><a href="admin/delete-user/{{this._id}}" onclick="return confirm('Are you want to delete {{this.name}}')" class="btn btn-danger">Delete</a></td>
                </tr>
                {{/each}}
            </tbody>
        </table>